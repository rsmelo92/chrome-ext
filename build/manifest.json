{
  "manifest_version": 2,
  "name": "Linkedin Ext",
  "version": "0.1",
  "description": "An extension to test code",
  "permissions": [
  ],
  "background": {
    "scripts": ["static/js/background.js"]
  },
  "content_scripts": [{
    "js": [ "static/js/content.js" ],
    "matches": [
      "http://*.example.com/*", "https://*.example.com/*"
    ]
  }],
  "icons": {
    "192": "logo-2x.png"
  },
  "browser_action": {
    "default_icon": {
      "192": "logo-2x.png"
    },
    "default_title": "Popup",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html"
  }
}
