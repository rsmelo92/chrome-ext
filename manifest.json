{
  "manifest_version": 3,
  "name": "ext",
  "version": "1.0",
  "description": "LinkedIn ext",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "icons": {
    "192": "assets/logo-2x.png"
  },
  "content_scripts": [{
    "matches": ["https://*.linkedin.com/in/*"],
    "js": ["src/content_script.js"]
  }]
}
